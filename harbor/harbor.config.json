    "rest": {
      "base_url": "https://api.harbor-dev.xyz/api/v1",
      "api_key_env": "HARBOR_API_KEY",
      "default_time_in_force": "gtc",
      "timeout": 20,
      "proxy": "http://127.0.0.1:7890",
      "trust_env": false,
