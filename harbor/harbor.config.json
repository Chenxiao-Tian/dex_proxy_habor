{
  "logging": {
    "level": "debug",
    "overrides": [
      {
        "logger_name": "aiohttp.access",
        "level": "warning"
      },
      {
        "logger_name": "pantheon.app_health",
        "level": "info"
      }
    ]
  },
  "server": {
    "host": "0.0.0.0",
    "port": 2958
  },
  "RabbitMQ": {
    "url": "amqp://localhost"
  },
  "key_store_file_path": "kuru/test-local-wallet.json",
  "dex": {
    "name": "harbor",
    "env": "stagenet",
    "base_url": "https://api.harbor-dev.xyz/api/v1",
